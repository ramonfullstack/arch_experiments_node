{
  "name": "ts-boilerplate",
  "version": "2.0.0",
  "main": "dist/index.js",
  "license": "UNLICENSED",
  "private": true,
  "type": "module",
  "author": "danitt <dan@staykind.io>",
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danitt/ts-boilerplate.git"
  },
  "scripts": {
    "start": "tsx src/server/index",
    "build": "npm run clean && tsc --project tsconfig.build.json",
    "clean": "rm -rf dist",
    "format": "npm run lint -- --fix",
    "lint": "eslint --config eslint.config.mjs --max-warnings=0 .",
    "lint:tsc": "tsc --noEmit",
    "test:unit": "tsx ./node_modules/mocha/bin/mocha \"**/*.spec.ts\"",
    "test:watch": "vitest --watch"
  },
  "dependencies": {
    "@eslint/js": "9.23.0",
    "@types/eslint__js": "^9.14.0",
    "@types/node": "^22.13.11",
    "@vitest/coverage-v8": "^3.0.9",
    "cors": "^2.8.5",
    "eslint": "9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-import-resolver-typescript": "^4.2.2",
    "eslint-plugin-import-x": "^4.9.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-unused-imports": "^4.1.4",
    "express": "^4.21.2",
    "prettier": "^3.5.3",
    "tsx": "^4.19.3",
    "typescript": "~5.8.2",
    "typescript-eslint": "^8.27.0",
    "vitest": "^3.0.9",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/chai": "^5.2.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/mocha": "^10.0.10",
    "chai": "^5.2.0",
    "mocha": "^11.1.0"
  }
}
